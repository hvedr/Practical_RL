      "    ) a\n",
      ";\n",
      "\n",
      "drop table if exists user_kposminin.user_urlp_train28;\n",
      "\n",
      "create table user_kposminin.user_urlp_train28 as \n",
      "   select \n",
      "        concat(cookie, 0) as cookie,\n",
      "        domain as urlp    \n",
      "    from user_kposminin.urls_w_levels_train28\n",
      "    union all\n",
      "    select\n",
      "        concat(cookie, 0) as cookie,\n",
      "        concat(domain,'[0]',lev0) as urlp\n",
      "     from user_kposminin.urls_w_levels_train28\n",
      "    union all\n",
      "    select \n",
      "        concat(cookie, 0) as cookie,\n",
      "        concat(domain,'[1]',lev1) as urlp\n",
      "    from user_kposminin.urls_w_levels_train28\n",
      "    union all\n",
      "    select \n",
      "        concat(cookie, 0) as cookie,\n",
      "        concat(domain,'[2]',lev2) as urlp\n",
      "    from user_kposminin.urls_w_levels_train28\n",
      ";\n",
      "\n",
      "drop table if exists user_kposminin.user_urlp_test28;\n",
      "\n",
      "create table user_kposminin.user_urlp_test28 as \n",
      "   select \n",
      "        concat(cookie, 1) as cookie,\n",
      "        domain as urlp    \n",
      "    from user_kposminin.urls_w_levels_test28\n",
      "    union all\n",
      "    select\n",
      "        concat(cookie, 1) as cookie,\n",
      "        concat(domain,'[0]',lev0) as urlp\n",
      "     from user_kposminin.urls_w_levels_test28\n",
      "    union all\n",
      "    select \n",
      "        concat(cookie, 1) as cookie,\n",
      "        concat(domain,'[1]',lev1) as urlp\n",
      "    from user_kposminin.urls_w_levels_test28\n",
      "    union all\n",
      "    select \n",
      "        concat(cookie, 1) as cookie,\n",
      "        concat(domain,'[2]',lev2) as urlp\n",
      "    from user_kposminin.urls_w_levels_test28   \n",
      ";\n",
      "\n",
      "drop table if exists user_kposminin.user_train28;\n",
      "\n",
      "create table user_kposminin.user_train28 as\n",
      "select\n",
      "   concat(id, 0)  as cookie,\n",
      "   max(case when ( url like \"%raiffeisen.ru/retail/cards/credit%\" ) then 1 else 0 end) as label\n",
      "from \n",
      "   user_kposminin.access_log_sample6 al\n",
      "   where ymd between \"2016-05-05\" and \"2016-05-05\"\n",
      "group by id;\n",
      "\n",
      "\n",
      "\n",
      "drop table if exists user_kposminin.user_test28;\n",
      "\n",
      "create table user_kposminin.user_test28 as\n",
      "select\n",
      "   concat(id, 1) as cookie,\n",
      "   max(case when ( url like \"%raiffeisen.ru/retail/cards/credit%\" ) then 1 else 0 end) as label\n",
      "from \n",
      "   user_kposminin.access_log_sample6 al\n",
      "   where ymd = \"2016-05-12\"\n",
      "group by id;\n",
      "\n",
      "\n",
      "drop table if exists user_kposminin.urlp_score_train28;\n",
      "\n",
      "create table user_kposminin.urlp_score_train28 as\n",
      "select\n",
      "    urlp,\n",
      "    log((positives + 0.5) / (total - positives + 0.5)) as score,\n",
      "    positives,\n",
      "    total\n",
      "from\n",
      "    (select\n",
      "        urlp,\n",
      "        sum(label) as positives,\n",
      "        count(cookie) as total\n",
      "    from\n",
      "        (select distinct\n",
      "            a.urlp,\n",
      "            a.cookie,\n",
      "            b.label\n",
      "        from \n",
      "            (select * \n",
      "             from user_kposminin.user_urlp_train28\n",
      "             where not ( urlp like \"%raiffeisen.ru/retail/cards/credit%\" or urlp like \"%raiffeisen.ru%\" )\n",
      "            ) a\n",
      "        left join user_kposminin.user_train28 b \n",
      "        on a.cookie = b.cookie\n",
      "        ) c\n",
      "    group by urlp\n",
      "    ) d\n",
      "where\n",
      "    total > 100\n",
      "    or positives > 5\n",
      ";\n",
      "\n",
      "\n",
      "drop table if exists user_kposminin.user_score_test28;\n",
      "\n",
      "create table user_kposminin.user_score_test28 as\n",
      "select\n",
      "    cs.cookie,\n",
      "    cs.score,\n",
      "    i.label\n",
      "from\n",
      "    (select \n",
      "        u.cookie,\n",
      "        max(s.score) as score\n",
      "    from \n",
      "        user_kposminin.user_urlp_test28 u\n",
      "    join user_kposminin.urlp_score_train28 s\n",
      "    on u.urlp = s.urlp\n",
      "    group by u.cookie) cs\n",
      "join user_kposminin.user_test28 i\n",
      "on i.cookie = cs.cookie;\n",
      "\n",
      "drop table if exists user_kposminin.user_score_test_exper28;\n",
      "\n",
      "create table user_kposminin.user_score_test_exper28 as\n",
      "select\n",
      "    cs.cookie,\n",
      "    cs.max_score,\n",
      "    cs.sum_score,\n",
      "    cs.avg_score,\n",
      "    cs.scores_list,\n",
      "    i.label    \n",
      "from\n",
      "    (select \n",
      "        u.cookie,\n",
      "        max(s.score) as max_score,\n",
      "        sum(s.score) as sum_score,\n",
      "        avg(s.score) as avg_score,\n",
      "        sort_array(collect_list(s.score)) as scores_list\n",
      "    from \n",
      "        user_kposminin.user_urlp_test28 u\n",
      "    join (\n",
      "      select * from user_kposminin.urlp_score_train28\n",
      "      order by score desc\n",
      "    ) s\n",
      "    on u.urlp = s.urlp\n",
      "    group by u.cookie) cs\n",
      "join user_kposminin.user_test28 i\n",
      "on i.cookie = cs.cookie\n"
     ]
    }
   ],
   "source": [
    "print(create_tables_in_hive_query)"
   ]
  },
